#!/usr/bin/env node

var conf = require('../lib/conf');
var S3Latest = require('../');

var stream = S3Latest({
  bucket: conf.get('bucket'),
  prefix: conf.get('prefix'),
  before: conf.get('before')
});

stream.on('error', function(err) {
  console.error(err.toString());
  process.exit(1);
});

stream.pipe(process.stdout);

